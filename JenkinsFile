pipeline{
  agent any
  stages{
    stage("Build"){
      steps {
        echo 'running code'
        bat 'python main.py'
      }
    }
   stage("Test"){
      steps{
        echo 'testing .......'
      }
    }
   stage("Deployment"){
      steps{
        echo 'deploying .......'
      }
    }
  }
}
